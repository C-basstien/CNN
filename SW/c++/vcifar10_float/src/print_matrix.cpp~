#include "../headers/main.hpp"
template <class  T>
T* print_matrix(T* matrix,unsigned int * dim_val_array, unsigned int  dim_size_array)
{
    if(dim_size_array>1)
    {
		for (int i = 0; i < dim_val_array[0];i++)
		{
		    matrix[i] = print_matrix(matrix[i],(dim_val_array+1),dim_size_array-1); 
		}
	}
	else
	{	cout<<"["
		for (int i = 0; i < dim_val_array[0];i++)
		{
		    cout<<matrix[i]<<"";
		}
		cout<<"]"<<endl;
	}
}
